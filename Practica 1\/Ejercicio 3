            {
                for(int i=0; i<n/2; i++){
                    temp=a[i];
                    a[i]= a[n-i-1];
                    a[n-i-1]= temp;
                }
                printf("El arreglo invertido es: \n");
                for(int i=0; i<n; i++){
                    printf("%d \n", a[i]);
                }
                break;
            }
